{
  "name": "cabal-bn-be",
  "version": "1.0.0",
  "description": "Cabal Barefoot Nomad Backend",
  "main": "src/app.js",
  "engines": {
    "yarn": "1.x"
  },
  "scripts": {
    "dev": "nodemon --exec npx babel-node src/app.js",
    "build": "babel src -d build",
    "start": "node build/app.js",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "test": {
      "command": "nyc --reporter=html --reporter=text mocha --require @babel/register ./tests/* --timeout 1000000 --exit || true",
      "env": {
        "NODE_ENV": "",
        "PORT": 3000
      }
    },
    "cov": {
      "command": "nyc report --reporter=text-lcov | coveralls"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/cabal-bn-be.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/cabal-bn-be/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/cabal-bn-be#readme",
  "devDependencies": {
    "@babel/node": "^7.16.8",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "better-npm-run": "^0.1.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mocha": "^9.2.1",
    "mongoose": "^6.2.4",
    "nyc": "^15.1.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "sinon": "^13.0.1",
    "supertest": "^6.2.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  }
}
