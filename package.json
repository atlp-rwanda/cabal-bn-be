{
  "name": "cabal-bn-be",
  "version": "1.0.0",
  "description": "Cabal Barefoot Nomad Backend",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec npx babel-node src/app.js",
    "build": "babel src -d build",
    "start": "node build/app.js",
    "dbmigrate": "npx sequelize-cli db:migrate",
    "seeding": "npx sequelize db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run dbmigrate",
    "dbmigrate:all": "npx sequelize-cli db:migrate:undo:all && npm run dbmigrate && npm run seeding"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/cabal-bn-be.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/cabal-bn-be/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/cabal-bn-be#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  }
}
